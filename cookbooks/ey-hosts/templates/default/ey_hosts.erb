#---EY-HOSTS-START
<% if @db_master.length > 0 %>
# EY Database Master
<%= @db_master %>         ey-db_master
<% end %>


<% if @db_replicas.length > 0 %>
# EY Database Replicas
<% end %>
<% @db_replicas.each do |key, value| %>
  <% if value.is_a? String %>
<%= value %>        <%= key %>
  <% else %>
    <% value.each_with_index do |host, index| %>
<%= host %>        <%= key %>-<%= index %>
    <% end %>
  <% end %>
<% end %>

<% if @utility_nodes.length > 0 %>
# EY Utility Instances
<% end %>
<% @utility_nodes.each do |key, value| %>
  <% if value.is_a? String %>
<%= value %>        <%= key %>
  <% else %>
    <% value.each_with_index do |host, index| %>
<%= host %>        <%= key %>-<%= index %>
    <% end %>
  <% end %>
<% end %>

    
#---EY-HOSTS-END
